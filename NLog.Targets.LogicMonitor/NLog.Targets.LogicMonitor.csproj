<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>NLogs.Targets.LogicMonitor</PackageId>
    <Version>0.0.1-alpha</Version>
    <RepositoryUrl>https://github.com/logicmonitor/nlog-target-logicmonitor</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>Logs generated by NLogs are ingested directly to LogicMonitor Platform.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Logicmonitor.DataSDK" Version="2.1.6-alpha" />
    <PackageReference Include="NLog" Version="5.0.5" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Logicmonitor.DataSDK" />
  </ItemGroup>
</Project>